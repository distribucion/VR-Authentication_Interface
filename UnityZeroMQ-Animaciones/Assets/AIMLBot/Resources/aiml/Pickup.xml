<?xml version="1.0" encoding="utf-8"?>
<aiml>
	<category>
		<pattern>*</pattern>
		<template>
			<srai>FRASE DE LIGUE AL AZAR</srai>
			<think>
				<set name="it">
					<set name="topic">
						<person/>
					</set>
				</set>
				<srai>
					PUSH <get name="topic"/>
				</srai>
			</think>
		</template>
	</category>

	<category>
		<pattern>FRASE DE LIGUE AL AZAR</pattern>
		<template>
			<random>
				<li>
					<srai>
						TIENE PREGUNTA <get name="has"/>
					</srai>
				</li>
				<li>
					<srai>
						TIENE PREGUNTA <get name="does"/>
